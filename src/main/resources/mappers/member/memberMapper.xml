<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.co.dbinc.com.web.repository.member.MemberMyBatisRepository">
    <insert id="createMember" parameterType="MemberRequest">
        INSERT INTO MEMBER(CITY,
                           STREET,
                           ZIPCODE,
                           NAME)
        VALUES(#{city},
               #{street},
               #{zipcode},
               #{name})
    </insert>
</mapper>